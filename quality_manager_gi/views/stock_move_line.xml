<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_stock_move_line_operation_tree_gi" model="ir.ui.view">
           <field name="name">stock.move.line.operations.tree</field>
            <field name="model">stock.move.line</field>
            <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree/field[@name='lot_name']" position="after">
                    <field name="visible_field" invisible="1"/>
                    <field name="supplier_lot" invisible="context.get('show_field')!='incoming'"/>
                </xpath>
                <xpath expr="//tree/field[@name='lot_name']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

            </field>
        </record>





