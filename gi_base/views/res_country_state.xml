<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>

        <!-- RES COUNTRY STATE INHERIT VIEWS -->

        <record id="res_country_state_gi_base_form" model="ir.ui.view">
            <field name="name">res.country.state.gi.base.form</field>
            <field name="model">res.country.state</field>
            <field name="inherit_id" ref="base.view_country_state_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='code']" position="after">
                    <field name="key"/>
                </xpath>

            </field>
        </record>

    </data>
</openerp>
