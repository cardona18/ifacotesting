<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="product_lot_make_date" model="ir.ui.view">
            <field name="name">product.lot.make.date</field>
            <field name="model">stock.production.lot</field>
            <field name="inherit_id" ref="stock.view_production_lot_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='alert_date']" position="before">
                    <field name="make_date"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>
